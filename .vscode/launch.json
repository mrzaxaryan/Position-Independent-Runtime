{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "i386-pc-windows-gnu (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "i386",
                "${workspaceFolder}/build/release/windows/i386/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "i386-pc-windows-gnu-release"
        },
        {
            "name": "x86_64-pc-windows-gnu (PIC)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/loader.py",
            "args": [
                "--arch",
                "x86_64",
                "${workspaceFolder}/build/release/windows/x86_64/output.bin"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "x86_64-pc-windows-gnu-release"
        },
        {
            "name": "aarch64-pc-windows-gnu (PIC)",
